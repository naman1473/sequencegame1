import random

cards = []
suits = ["Hearts", "Diamonds", "Clubs", "Spades"]
royals = ["Jack", "Queen", "King", "Ace"]
carddeck = []

for i in range(2,11):
    cards.append(str(i))
for a in range(4):
    cards.append(royals[a])
for c in range(4):
    for d in range(13):
        card1 = cards[d] + " of " + suits[c]
        carddeck.append(card1)

random.shuffle(carddeck)

for g in range(52):
    print(carddeck[g])
